{
  "name": "divante-ltd/orocommerce-elasticsearch-bundle",
  "description": "Elasticsearch Bundle for Oro Commerce",
  "type": "orocommerce-module",
  "license": "MIT",
  "authors": [
    {
      "name": "Piotr Belina",
      "email": "belina.piotr@gmail.com"
    }
  ],
  "repositories": {
    "composer": {
      "type": "composer",
      "url": "https://packagist.orocrm.com"
    },
    "asset-packagist": {
      "type": "composer",
      "url": "https://asset-packagist.orocrm.com"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "oro/platform-serialised-fields": "6.0.*",
    "oro/commerce": "6.0.*",
    "ruflin/elastica": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.5.27"
  },
  "autoload": {
    "psr-4": {
      "Divante\\Bundle\\ElasticsearchBundle\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true,
      "symfony/flex": true,
      "php-http/discovery": true
    }
  }
}
